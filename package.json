{
    "name": "icevent_ui",
    "version": "0.1.0",
    "author": "Orcsun",
    "description": "An open event service",
    "keywords": [
        "event",
        "calendar",
        "todo",
        "schedule",
        "NFT"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "next build && next export",
        "start": "next start",
        "lint": "next lint",
        "export": "next export",
