    },
    "dependencies": {
        "@dfinity/agent": "^0.9.2",
        "@dfinity/candid": "^0.9.2",
        "@dfinity/principal": "^0.9.2",
        "assert": "^2.0.0",
        "buffer": "^6.0.3",
        "events": "^3.3.0",
        "moment": "^2.29.1",
        "next": "^11.0.1",
        "react": "^17.0.2",
        "react-big-calendar": "^0.38.0",
        "react-dom": "^17.0.2",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^2.0.4",
        "stream-browserify": "^3.0.0",
        "util": "^0.12.4"
